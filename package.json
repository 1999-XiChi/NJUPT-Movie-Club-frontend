{
  "name": "NJUPTMovieClub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start:dev": "vue-cli-service serve --mode=development --open",
    "start:prod": "vue-cli-service build --mode=production",
    "start:test": "vue-cli-service build --mode=test",
    "mock": "json-server --watch src/mock/db.json --port 3000 --routes src/mock/routes.json",
    "mockdev": "yarn mock | yarn start:dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "babel-polyfill": "^6.26.0",
    "compression-webpack-plugin": "^3.1.0",
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg-sprite-loader": "^4.2.1",
    "uglifyjs-webpack-plugin": "1.1.1",
    "vue": "^2.6.10",
    "vue-image-crop-upload": "^2.5.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "deepmerge": "^4.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.13.0",
    "fibers": "^4.0.2",
    "mockjs": "^1.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
